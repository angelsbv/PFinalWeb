    <script src="~/Resources/leaflet/dist/leaflet.js"></script>
    <script src="~/Resources/leaflet/dist/leaflet-extra-markers.min.js"></script>
    <script>
        $(document).ready(function () { $('textarea#Contenido').characterCounter(); });
        $(document).ready(function () { $('input#Resumen').characterCounter(); });
    </script>
    <script>
        $(window).on("load", function () {
            $(".loader-wrapper").fadeOut("slow");
        });
    </script>
</body>
</html>
