<!DOCTYPE html>
<html>
<head>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/materialize.min.css" />
    <link rel="stylesheet" href="~/Content/style.css" />
    <link rel="stylesheet" href="~/Resources/leaflet/dist/leaflet.css" />
    <link rel="stylesheet" href="~/Resources/leaflet/dist/leaflet-extra-markers.min.css" />
    <style>.sidenav {z-index: 1025 !important;}</style>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Covid19 Info</title>
</head>
<body onload="footerSpns();">
    <div class="loader-wrapper">
        <span class="loader"><span class="loader-inner"></span></span>
    </div>
    <header>
        <nav style="margin-top:0px!important;">
            <div class="nav-wrapper blue darken-2">
                <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
                <ul class="hide-on-med-and-down" style="margin-left:15.2%;">
                    @*==>*@
                    <li class="left unableToSelect" style="margin-left:-15.3%!important;">
                        <a class="grey-text text-lighten-4 spnFooter" href="#!"></a>
                    </li>
                    <li>
                        <a href="~/home/index" title="Inicio">
                            <i class="material-icons">home</i>
                        </a>
                    </li>
                    <li>
                        <a href="~/home/mapa" title="Mapa">
                            <i class="material-icons">map</i>
                        </a>
                    </li>
                    <li>
                        <a href="~/home/news" title="Noticias">
                            <i class="material-icons">fiber_new</i>
                        </a>
                    </li>
                    <li>
                        <a href="~/home/stats" title="Estadisticas">
                            <i class="material-icons">insert_chart</i>
                        </a>
                    </li>
                    <li>
                        <a href="~/home/casos" title="Casos">
                            <i class="material-icons">local_hospital</i>
                        </a>
                    </li>
                    <li>
                        <a href="~/home/suscribete">
                            Suscribete
                            <i class="material-icons right">textsms</i>
                        </a>
                    </li>
                    @if (!Request.IsAuthenticated)
                    {
                        <li class="right" style="margin-right:19%;">
                            <a href="~/admin/login">Iniciar Sesión (Admin)</a>
                        </li>
                    }
                    else
                    {
                        using (Html.BeginForm("Logout", "Admin", FormMethod.Post, new { id = "logoutForm" }))
                        {
                            <li class="right" style="margin-right:19%;">
                                <a href="javascript:if(confirm('¿Seguro que desea cerrar sesión?'))document.querySelector('#logoutForm').submit()">Cerrar sesión</a>
                            </li>
                        }
                        <li class="right">
                            <a href="~/admin/adminpc">
                                @HttpContext.Current.User.Identity.Name <i class="small material-icons right yellow-text" title="Panel de Control">verified_user</i>
                            </a>
                        </li>
                    }
                </ul>
            </div>
        </nav>
        <ul class="sidenav collection with-header" id="mobile-demo">
            <li class="collection-header"><h4 class="spnFooter"></h4></li>
            <li class="collection-item">
                <a href="~/home/index">
                    Inicio
                    <span class="secondary-content"><i class="material-icons">send</i></span>
                </a>
            </li>
            <li class="collection-item">
                <a href="~/home/mapa">
                    Mapa
                    <span class="secondary-content"><i class="material-icons">send</i></span>
                </a>
            </li>
            <li class="collection-item">
                <a href="~/home/news">
                    Noticias
                    <span class="secondary-content"><i class="material-icons">send</i></span>
                </a>
            </li>
            <li class="collection-item">
                <a href="~/home/suscribete">
                    Suscribete
                    <span class="secondary-content"><i class="material-icons">send</i></span>
                </a>
            </li>
            <li class="collection-item">
                <a href="~/home/casos">
                    Casos
                    <span class="secondary-content"><i class="material-icons">send</i></span>
                </a>
            </li>
            <li class="collection-item">
                <a href="~/home/stats">
                    Estadisticas
                    <span class="secondary-content"><i class="material-icons">send</i></span>
                </a>
            </li>
            @if (!Request.IsAuthenticated)
            {
                <li class="collection-item">
                    <a href="~/admin/login">
                        Iniciar Sesión (Admin)
                        <span class="secondary-content"><i class="material-icons">send</i></span>
                    </a>
                </li>
            }
            else
            {
                <li class="collection-item">
                    <a href="~/admin/adminpc">
                        @HttpContext.Current.User.Identity.Name (Panel de Control)
                    </a>
                </li>
            }

        </ul>
    </header>
    <main class="container body-content">
        @RenderBody()
    </main>
    <footer class="page-footer blue darken-2">
        <div class="footer-copyright">
            <div class="container">
                &copy; @DateTime.Now.Year Copyright
            </div>
        </div>
    </footer>
    @*<footer class="page-footer teal">
            <div class="container">
                <div class="row">
                    <div class="col l6 s12">
                        <h5 class="white-text">Footer Content</h5>
                        <p class="grey-text text-lighten-4">Rows & cols.</p>
                    </div>
                </div>
            </div>
            <div class="footer-copyright">
                <div class="container">
                    &copy; @DateTime.Now.Year Copyright
                    <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
                </div>
            </div>
        </footer>*@
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    <script src="~/Scripts/materialize.min.js"></script>
